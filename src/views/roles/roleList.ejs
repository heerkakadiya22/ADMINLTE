<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>AdminLTE | Role List</title>
    <%- include('partials/head') %>
    <script
      src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <link rel="stylesheet" href="/src/assets/css/style.css">
  </head>
  <body class="hold-transition sidebar-mini">
    <div class="wrapper">
      <%- include('partials/sidebar', { name: name, image: image, roleId:
      roleId,
      csrfToken: csrfToken }) %>

      <div class="content-wrapper">
        <%- include('partials/contentHeader', { pageTitle: pageTitle,
        breadcrumbs: breadcrumbs }) %>

        <section class="content">
          <div class="container-fluid">
            <div class="card">
              <div class="card-body p-0">

                <table class="table table-striped"
                  id="rolesTable">
                  <thead class="thead-dark">
                    <tr>
                      <th>#</th>
                      <th>Name</th>
                      <th>Active</th>
                      <th>Description</th>
                      <th>Actions</th>
                    </tr>
                  </thead>
                  <tbody id="rolesTableBody">
                    <tr><td colspan="5" class="text-center text-muted">Loading
                        roles...</td></tr>
                  </tbody>

                </table>
              </div>
            </div>
          </div>
        </section>
      </div>
    </div>

    <%- include('partials/scripts') %>
    <script src="/src/assets/jquery/ajaxJquery.js"></script>
  </body>
</html>
